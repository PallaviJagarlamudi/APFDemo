<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.apfdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DashBoardActivity"
            android:label="@string/category_dashboard" />
        <activity
            android:name=".BachrolActivity"
            android:label="@string/category_bachrol" />
        <activity
            android:name=".BundlePatchActivity"
            android:label="@string/category_bundlepatch" />
        <activity
            android:name=".CPMActivity"
            android:label="@string/category_cpm" />
        <activity
            android:name=".FAActivity"
            android:label="@string/category_fa" />
        <activity
            android:name=".ReportsActivity"
            android:label="@string/category_reports" />
        <activity android:name=".DBFirstActivity"
            android:label="@string/db_one"/>
        <activity android:name=".DBSecondActivity"
            android:label="@string/db_second" />
        <activity android:name=".DBThirdActivity"
            android:label="@string/db_third"></activity>
    </application>

</manifest>